{
  "name": "i18next-utilities-ts-cli",
  "version": "0.1.0",
  "description": "Generate i18next type definitions from your resource files.",
  "main": "dist/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "bin": {
    "i18next-ts": "dist/index.js"
  },
