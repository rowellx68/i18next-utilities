{
  "name": "i18next-utilities-unplugin",
  "version": "0.1.1",
  "description": "A collection of i18next plugins for Vite, Rollup, Webpack, Farm and esbuild",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "test": "vitest run",
    "format": "prettier --write .",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "types": "./dist/rollup.d.ts"
    },
    "./farm": {
      "import": "./dist/farm.js",
      "types": "./dist/farm.d.ts"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "types": "./dist/esbuild.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "i18next",
    "unplugin",
    "vite",
    "rollup",
    "webpack",
    "esbuild",
    "farm",
    "localization",
    "internationalization"
  ],
  "author": "Rowell Heria",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rowellx68/i18next-utilities.git"
  },
  "devDependencies": {
    "@farmfe/core": "^1.1.6",
    "@types/node": "^20.12.12",
    "esbuild": "^0.20.2",
    "i18next": "^23.11.4",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "webpack": "^5.91.0"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "i18next-utilities-core": "workspace:*",
    "unplugin": "^1.10.1"
  },
  "peerDependencies": {
    "@farmfe/core": "^1.1.6",
    "esbuild": "^0.20.2",
    "i18next": "^23.11.4",
    "rollup": "^4.17.2",
    "vite": "^5.2.11",
    "webpack": "^5.91.0"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}
